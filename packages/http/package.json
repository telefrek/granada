{
  "name": "@telefrek/http",
  "version": "1.0.0-alpha.13",
  "description": "This package standardizes how HTTP clients and servers interact at a low level",
  "scripts": {
    "clean": "rm -rf ./dist",
    "upgrade": "npx npm-check-updates -u && npm install",
    "build": "tsc",
    "compile": "tsc -p tsconfig.compile.json",
    "lint": "npx eslint . --fix"
  },
  "keywords": [
    "granada",
    "framework",
    "http"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.json"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*/index.js",
      "./dist/*.js"
    ],
    "./*.js": "./dist/*.js"
  },
  "author": "nathan@telefrek.com",
  "license": "MIT",
  "dependencies": {
    "@telefrek/core": "^1.0.0-alpha.13"
  },
  "gitHead": "730d8573af6f7e7edf7187ab6f1a082f52d224e0"
}
