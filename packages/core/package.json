{
  "name": "@telefrek/core",
  "version": "1.0.0-alpha.17",
  "description": "This package contains the core utilities that are referenced throughout the Granada framework",
  "author": "nathan@telefrek.com",
  "license": "MIT",
  "keywords": [
    "granada",
    "structures",
    "concurrency"
  ],
  "type": "module",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.json"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./*.js": "./dist/*.js"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "upgrade": "npx npm-check-updates -u && npm install",
    "build": "tsc",
    "compile": "tsc -p tsconfig.compile.json",
    "lint": "npx eslint . --fix"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/semantic-conventions": "^1.23.0"
  },
  "devDependencies": {
    "@opentelemetry/core": "^1.23.0"
  },
  "gitHead": "1be6bb3a61aab8ae5361827b54f8428682b9d536"
}
