{
  "name": "@telefrek/service",
  "version": "1.0.0-alpha.17",
  "description": "This package makes creating new services easier than working with the raw HTTP stack",
  "scripts": {
    "clean": "rm -rf ./dist",
    "upgrade": "npx npm-check-updates -u && npm install",
    "build": "tsc",
    "compile": "tsc -p tsconfig.compile.json",
    "lint": "npx eslint . --fix"
  },
  "keywords": [
    "granada",
    "framework",
    "service",
    "REST"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.json"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*/index.js",
      "./dist/*.js"
    ],
    "./*.js": "./dist/*.js"
  },
  "author": "nathan@telefrek.com",
  "license": "MIT",
  "dependencies": {
    "@telefrek/http": "^1.0.0-alpha.17"
  },
  "gitHead": "1be6bb3a61aab8ae5361827b54f8428682b9d536"
}
