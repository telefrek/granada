{
  "name": "@telefrek/postgres",
  "version": "1.0.0-alpha.13",
  "description": "This package handles abstractions around postgres and interactions with a database",
  "scripts": {
    "clean": "rm -rf ./dist",
    "upgrade": "npx npm-check-updates -u && npm install",
    "build": "tsc",
    "compile": "tsc -p tsconfig.compile.json",
    "lint": "npx eslint . --fix"
  },
  "keywords": [
    "granada",
    "framework",
    "postgres"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.json"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*/index.js",
      "./dist/*.js"
    ],
    "./*.js": "./dist/*.js"
  },
  "author": "nathan@telefrek.com",
  "license": "MIT",
  "dependencies": {
    "@telefrek/core": "^1.0.0-alpha.13",
    "@telefrek/query": "^1.0.0-alpha.13",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.8.1",
    "@types/pg": "^8.11.4"
  },
  "gitHead": "983efc5d93194bc23bc5558e6bfdd28c35aa0165"
}
